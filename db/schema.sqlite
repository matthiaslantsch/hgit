CREATE TABLE "projectType" (
  -- projectType_1512584324
  "idProjectType" integer NOT NULL PRIMARY KEY,
  "name" varchar(40) NOT NULL
)

CREATE TABLE "project" (
  -- project_1512584326
  "idProject" integer NOT NULL PRIMARY KEY,
  "name" varchar(20) NOT NULL,
  "description" text,
  "major" varchar(10) NOT NULL,
  "minor" varchar(10) NOT NULL,
  "fix" varchar(10) NOT NULL,
  "otherMask" integer NOT NULL,
  "anyMask" integer NOT NULL DEFAULT 0,
  "idUser" integer NOT NULL,
  "idProjectType" integer,
  CONSTRAINT "unq_project_name" UNIQUE ("name"),
  FOREIGN KEY ("idUser") REFERENCES "user"("idUser"),
  FOREIGN KEY ("idProjectType") REFERENCES "projectType"("idProjectType")
)

CREATE TABLE "activityType" (
  -- activityType_1512584327
  "idActivityType" integer NOT NULL PRIMARY KEY,
  "name" varchar(10) NOT NULL
)

CREATE TABLE "activity" (
  -- activity_1512584328
  "idActivity" integer NOT NULL PRIMARY KEY,
  "content" text NOT NULL,
  "createtime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updatetime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "idUser" integer,
  "idProject" integer NOT NULL,
  "idActivityType" integer NOT NULL,
  FOREIGN KEY ("idUser") REFERENCES "user"("idUser"),
  FOREIGN KEY ("idProject") REFERENCES "project"("idProject"),
  FOREIGN KEY ("idActivityType") REFERENCES "activityType"("idActivityType")
)

CREATE TABLE "issueType" (
  -- issueType_1512584329
  "idIssueType" integer NOT NULL PRIMARY KEY,
  "name" varchar(10) NOT NULL
)

CREATE TABLE "issueStatus" (
  -- issueStatus_1512584330
  "idIssueStatus" integer NOT NULL PRIMARY KEY,
  "name" varchar(10) NOT NULL
)

CREATE TABLE "issue" (
  -- issue_1512584331
  "idIssue" integer NOT NULL PRIMARY KEY,
  "title" varchar(40) NOT NULL,
  "description" text NOT NULL,
  "targetVersion" varchar(15),
  "idProject" integer NOT NULL,
  "author" integer NOT NULL,
  "idIssueType" integer NOT NULL,
  "idIssueStatus" integer NOT NULL,
  "createtime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updatetime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  FOREIGN KEY ("idProject") REFERENCES "project"("idProject"),
  FOREIGN KEY ("author") REFERENCES "user"("idUser"),
  FOREIGN KEY ("idIssueType") REFERENCES "issueType"("idIssueType"),
  FOREIGN KEY ("idIssueStatus") REFERENCES "issueStatus"("idIssueStatus")
)

CREATE TABLE "comment" (
  -- comment_1512584332
  "idComment" integer NOT NULL PRIMARY KEY,
  "description" text NOT NULL,
  "createtime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updatetime" timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "idIssue" integer NOT NULL,
  "idUser" integer NOT NULL,
  FOREIGN KEY ("idIssue") REFERENCES "issue"("idIssue"),
  FOREIGN KEY ("idUser") REFERENCES "user"("idUser")
)

CREATE TABLE "group" (
  -- group_1512584333
  "idGroup" integer NOT NULL PRIMARY KEY,
  "name" varchar(60) NOT NULL
)

CREATE TABLE "group2user" (
  -- group2user_1512584334
  "idGroup" integer NOT NULL,
  "idUser" integer NOT NULL,
  FOREIGN KEY ("idGroup") REFERENCES "group"("idGroup"),
  FOREIGN KEY ("idUser") REFERENCES "user"("idUser"),
  CONSTRAINT "pkey_group2user" PRIMARY KEY ("idGroup", "idUser")
)

CREATE TABLE "userAccess" (
  -- userAccess_1512584335
  "idUserAccess" integer NOT NULL PRIMARY KEY,
  "mask" integer NOT NULL,
  "idProject" integer NOT NULL,
  "idUser" integer NOT NULL,
  FOREIGN KEY ("idProject") REFERENCES "project"("idProject"),
  FOREIGN KEY ("idUser") REFERENCES "user"("idUser")
)

CREATE TABLE "groupAccess" (
  -- groupAccess_1512584336
  "idGroupAccess" integer NOT NULL PRIMARY KEY,
  "mask" integer NOT NULL,
  "idProject" integer NOT NULL,
  "idGroup" integer NOT NULL,
  FOREIGN KEY ("idProject") REFERENCES "project"("idProject"),
  FOREIGN KEY ("idGroup") REFERENCES "group"("idGroup")
)

CREATE TABLE "user" (
  -- user_1535368976
  "idUser" integer NOT NULL PRIMARY KEY,
  "name" varchar(40) NOT NULL,
  "nick" varchar(20) NOT NULL,
  "email" varchar(60)
)